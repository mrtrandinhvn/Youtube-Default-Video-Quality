{
    "name": "Youtube Default Video Quality",
    "version": "2.0.6",
    "description": "This is an extension for Chrome base web browsers. It allows users to set default video quality for youtube.com",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "js/contentscriptjs/commonfns.js",
                "js/contentscriptjs/main.js"
            ]
        }
    ],
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/icon-16.png",
            "32": "img/icon-32.png",
            "48": "img/icon-48.png"
        },
        "default_popup": "default_popup.html"
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
    ],
    "manifest_version": 2,
    "author": "goseidinh"
}
